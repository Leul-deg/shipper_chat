{
  "name": "shipper-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--dns-result-order=ipv4first' tsx watch server.ts",
    "dev:next": "next dev",
    "build": "next build",
    "build:server": "tsc server.ts --outDir . --esModuleInterop --module commonjs --target ES2017 --skipLibCheck --resolveJsonModule",
    "build:all": "npm run build && npm run build:server",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "lint": "eslint",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "docker:build": "sudo docker build -t shipper-chat .",
    "docker:run": "sudo docker run -p 3000:3000 --env-file .env.local shipper-chat"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^6.3.1",
    "better-auth": "^1.4.3",
    "dotenv": "^17.2.3",
    "next": "16.0.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.13",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "prisma": "^6.3.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
